{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/UserMenu.tsx", "components/UserMenu.jsx", "components/user-cards/ClientTrade.tsx", "components/user-cards/ClientTradejs.jsx", "components/user-cards/r.js", "app/[lang]/payment-result/page.jsx", "components/user-cards/ClientHistory.tsx", "components/user-cards/ClientHistory copy.jsx", "components/user-cards/ExpertList.js"],
  "exclude": ["node_modules"]
}
